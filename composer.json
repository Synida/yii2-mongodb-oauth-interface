{
  "name": "synida/yii2-mongodb-oauth-interface",
  "description": "Yii2 mongoDB OAuth2 interface",
  "minimum-stability": "stable",
  "homepage": "https://github.com/Synida/yii2-mongodb-oauth-interface",
  "license": "MIT",
  "authors": [
    {
      "name": "David Hlavati",
      "email": "david.hlavati@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-ctype": "*",
    "ext-mongodb": "*",
    "synida/yii2-oauth2-server": "~2.0",
    "yiisoft/yii2-mongodb": "^2.1"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
